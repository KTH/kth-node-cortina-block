    "email": "infosys@kth.se",
    "url": "https://github.com/KTH"
  },
  "license": "MIT",
  "dependencies": {
    "@kth/log": "^4.0.7",
    "kth-node-redis": "^3.3.0"
  },
  "devDependencies": {
    "@kth/eslint-config-kth": "^3.6.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.17",
    "eslint": "^8.57.1",
    "express": "^4.21.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
