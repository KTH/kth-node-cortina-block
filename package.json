{
  "name": "kth-node-cortina-block",
  "version": "3.1.0",
  "description": "Node.js module for fetching Cortina blocks and optionally cache using Redis.",
  "automaticPublish": "true",
  "main": "index.js",
  "scripts": {
    "test": "tape test/**/*Test.js | tap-spec",
    "codecheck": "standard",
    "standardfix": "standard --fix",
    "preversion": "npm run codecheck && npm run test",
    "postversion": "git push && git push --tags",
    "build": "./build.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KTH/kth-node-cortina-block"
  },
  "keywords": [
    "node",
    "kth",
    "cortina",
    "blocks"
  ],
  "author": {
    "name": "KTH",
    "email": "infosys@kth.se",
    "url": "https://github.com/KTH"
  },
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "lodash": "^4.17.20",
    "request": "2.88.2",
    "request-promise": "^4.2.6"
  },
  "peerDependencies": {
    "kth-node-log": "^3.1.2"
  },
  "devDependencies": {
    "mockery": "^2.1.0",
    "proxyquire": "^2.1.3",
    "standard": "^14.3.4",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  }
}
